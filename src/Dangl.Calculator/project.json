{
  "authors": [ "Georg Dangl" ],
  "copyright": "(c) 2016 Georg Dangl",
  "description": "Package to evaluate mathematical formulas from strings",
  "title": "Dangl.Calculator",
  "packOptions": {
    "projectUrl": "https://github.com/GeorgDangl/Dangl.Calculator",
    "licenseUrl": "https://github.com/GeorgDangl/Dangl.Calculator/blob/master/LICENSE.md",
    "owners": [ "Georg Dangl" ],
    "releaseNotes": "1.0.2: Changed dependencies to only need Antlr4.Runtime instead of the full Antlr4 package"
  },
  "version": "1.0.2",
  "dependencies": {
    "Antlr4.Runtime": "4.5.3.1-beta001",
    "Antlr4.CodeGenerator": {
      "version": "4.5.3.1-beta001",
      "type": "build"
    }
  },
  "frameworks": {
    "netcoreapp1.0": {
        "imports": [
            "dnxcore50",
            "portable-net45+win8"
        ],
        "dependencies": {
            "Microsoft.NETCore.App": {
                "version": "1.0.1",
                "type": "platform"
            }
        }
    },
    "net461": {}
  },
  "scripts": {
    "precompile": [
      "BuildANTLR.bat"
    ]
  },
  "buildOptions": {
    "xmlDoc": true
  }
